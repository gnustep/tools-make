  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: ignoring --with-user-dir=$OBSOLETE_GNUSTEP_USER_DIR option" >&5
$as_echo "$as_me: WARNING: ignoring --with-user-dir=$OBSOLETE_GNUSTEP_USER_DIR option" >&2;}
