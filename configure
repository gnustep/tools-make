$as_echo "$GNUSTEP_USER_DEFAULTS_DIR" >&6; }


#--------------------------------------------------------------------
# Setting up GNUSTEP_MAKEFILES
#--------------------------------------------------------------------
#
# This is where you install gnustep-make.  We just print it out.
#
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for GNUSTEP_MAKEFILES to use" >&5
$as_echo_n "checking for GNUSTEP_MAKEFILES to use... " >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $GNUSTEP_MAKEFILES" >&5
$as_echo "$GNUSTEP_MAKEFILES" >&6; }
# If GNUSTEP_MAKEFILES contains a space, we may have problems later
# because make does not really support having spaces in filenames
if echo "$GNUSTEP_MAKEFILES" | grep " " >/dev/null 2>&1; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: GNUSTEP_MAKEFILES ($GNUSTEP_MAKEFILES) contains spaces: this may not work" >&5
$as_echo "$as_me: WARNING: GNUSTEP_MAKEFILES ($GNUSTEP_MAKEFILES) contains spaces: this may not work" >&2;}
fi

#--------------------------------------------------------------------
# Setting up the install-sh script
#--------------------------------------------------------------------

# HOST_INSTALL is the name of the install program in config.make so set it up
# to point to the install-sh script in the GNUstep tree if no system install is
# found.

